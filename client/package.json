{
  "name": "react_project_base",
  "version": "1.0.0",
  "description": "base structure and webpack for react - typescript projects",
  "main": "index.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.config.dev.js",
    "build:dev": "webpack --mode development --config webpack.config.dev.js",
    "serve:dev": "npm run build:dev && serve:dev -s dist",
    "format": "prettier --write \"src/**/*.+(js|jsx)\"",
    "eslint-fix": "eslint --fix \"src/**/*.+(js|jsx)\""
  },
  "repository": {
    "type": "git",
    "url": "base_express_react"
  },
  "keywords": [
    "js",
    "react",
    "jsx",
    "typescript"
  ],
  "author": "Juan David Correa Cano",
  "license": "ISC",
  "dependencies": {
